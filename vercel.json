{
  "version": 2,
  "builds": [
    {
      "src": "model/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "requirements": "requirements.txt"
      }
    },
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "model/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
